# supabase
SUPABASE_URL=http://localhost:54321
SUPABASE_KEY=anon_key_placeholder
SUPABASE_SERVICE_ROLE_KEY=service_role_key_placeholder

# Google OAuth for Supabase external provider
SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com
SUPABASE_AUTH_EXTERNAL_GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret
# Example redirect URI to register in Google Console (use your Supabase API URL)
# e.g. http://127.0.0.1:54321/auth/v1/callback
SUPABASE_AUTH_EXTERNAL_GOOGLE_REDIRECT_URI=http://127.0.0.1:54321/auth/v1/callback

# server configs
PORT=3005
CORS_ORIGINS="http://localhost:3000,http://127.0.0.1:3000"

# database
DATABASE_URL="<YOUR_DATABASE_URL>"
